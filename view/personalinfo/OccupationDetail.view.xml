<core:View xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	xmlns="sap.m" controllerName="com.zhenergy.hr.view.personalinfo.OccupationDetail" 
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{QualificationName}" backgroundDesign="Solid" showNavButton="{device>/isPhone}" navButtonPress="back" showFooter="{device>/isPhone}">
		<content>
	        <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="证书编号" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{QualificationId}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="类型" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{Type}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
