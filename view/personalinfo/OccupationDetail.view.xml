            <VBox width="60%" >
                <Text text="{HighestGradeFlag}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="取得时间" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{IssueDate}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="评审机构" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{ReviewInstitution}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="颁发机构" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{IssueInstitution}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="获得资格途径" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{ComeFrom}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="批准文件" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{Document}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="聘任时间" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{HireDate}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="解聘时间" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{FireDate}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="聘任单位" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{HireCompany}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
            
            <HBox width="100%">
            <VBox width="40%" direction="Column" >
               <Text class="text" text="备注" width="100%" textAlign="Right"/>
            </VBox>
            <VBox width="60%" >
                <Text text="{Remark}" maxLines="0" textAlign="Left" class="vbox_text02"></Text>
            </VBox>
            </HBox>
