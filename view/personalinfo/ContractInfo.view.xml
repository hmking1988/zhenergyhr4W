<sap.ui.core.mvc:View controllerName="com.zhenergy.hr.view.personalinfo.ContractInfo"
    xmlns="sap.m"
    xmlns:sap.ui.core.mvc="sap.ui.core.mvc" >
    <Page title="合同要素" showNavButton="true" navButtonPress="back" >
        <content>
            <List
		    class="listclass"
			id="list"
			mode="{device>/listMode}"
			select="handlecontractdetailListItemSelect"
			>
			    <StandardListItem type="Navigation" counter="0" title="{ContractType}" press="handlecontractdetailListItemPress"></StandardListItem>
	    	</List>
        </content>
        <footer>
            <Toolbar>
                <content>
                <!--<ToolbarSpacer/>-->
                <Button icon="sap-icon://home" press="back1"></Button>
                </content>
            </Toolbar>
        </footer>
    </Page>
        
</sap.ui.core.mvc:View>